                <Button x:Name="Minimize" Content="-" Height="20" Width="20" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" FontSize="11" Margin="1160,0,20,0" Click="Minimize_Click"/>
            </Grid>
        </DockPanel>

